pipeline {
  agent {
    docker { image 'liquibase/liquibase:4.4.2' }
  }
  stages {
    stage('Rollback') {
      steps {
        sh 'liquibase --url="jdbc:postgresql://host.docker.internal:5432/testdb-1" --changeLogFile=/var/jenkins_home/workspace/liquibase_test/rollBackChangeLog.xml --username=postgres --password=secure_pass_here rollback version1'
      }
    }
  }
}