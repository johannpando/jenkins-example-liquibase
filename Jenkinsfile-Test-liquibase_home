pipeline {
  agent any
  environment {
    LIQUIBASE_HOME = tool name: 'Liquibase', type: 'org.jenkinsci.plugins.liquibase.install.LiquibaseInstallation'
  }
  stages {
    stage('Status') {
      steps {
        bat "${LIQUIBASE_HOME}//liquibase status --changelog-file=blank-changelog.sql"
      }
    }
  }
}